<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ParallaxImageView">
        <attr name="intensity" format="float" />
        <attr name="tiltSensitivity" format="float" />
        <attr name="forwardTiltOffset" format="float" />
    </declare-styleable>
    <declare-styleable name="FloatLabelEditText">
        <attr name="fitScreenWidth" format="enum">
            <enum name="full" value="1" />
            <enum name="half" value="2" />
        </attr>
        <attr name="gravity" format="enum">
            <enum name="left" value="3" />
            <enum name="right" value="5" />
            <enum name="center" value="17" />
        </attr>
        <attr name="hint" format="string" />
        <attr name="inputType" format="enum">
            <enum name="normal" value="0" />
            <enum name="textPassword" value="1" />
        </attr>
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColorHint" format="color" />
        <attr name="textColorFocussed" format="color" />
        <attr name="textColorUnFocussed" format="color" />
        <attr name="textColorFloatLabelFocussed" format="color" />
        <attr name="textColorFloatLabelUnFocussed" format="color" />
    </declare-styleable>

</resources>